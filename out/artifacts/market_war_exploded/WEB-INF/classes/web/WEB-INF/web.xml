<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>CategoryGameListServlet</servlet-name>

        <servlet-class>com.demo.servlet.CategoryGameListServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>CategoryGameListServlet</servlet-name>
        <url-pattern>/CategoryGameListServlet</url-pattern>
    </servlet-mapping>



    <servlet>


        <servlet-name>CategoryMovieListServlet</servlet-name>

        <servlet-class>com.demo.servlet.CategoryMovieListServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>CategoryMovieListServlet</servlet-name>
        <url-pattern>/CategoryMovieListServlet</url-pattern>
    </servlet-mapping>

   <servlet>
       <servlet-name>MovieListServlet</servlet-name>
       <servlet-class>com.demo.servlet.MovieListServlet</servlet-class>
   </servlet>
    <servlet-mapping>
        <servlet-name>MovieListServlet</servlet-name>
        <url-pattern>/MovieListServlet</url-pattern>
    </servlet-mapping>


</web-app>